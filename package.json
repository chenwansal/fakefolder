{
	"name": "fakefolder",
	"displayName": "FakeFolder",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.65.0"
	},
	"publisher": "jackwithtea",
	"author": {
		"name": "杰克有茶",
		"email": "chenwansal1@163.com",
		"url": "https://jackwithtea.notion.site/d0010115e393418bb65141d6cc027b87"
	},
	"repository": {
		"url": "https://github.com/chenwansal/fakefolder"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:RETreeview"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "fakefolder.create-fake-folder",
				"title": "create-fake-folder"
			},
			{
				"command": "fakefolder.put-file-to-fake-folder",
				"title": "put-file-to-fake-folder"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "REActiveBar",
					"title": "Resource Explorer",
					"icon": "media/icon.svg"
				}
			]
		},
		"views": {
			"explorer": [
				{
					"id": "RETreeview",
					"name": "RETreeview"
				}
			],
			"REActiveBar": [
				{
					"id": "REActiveBar",
					"name": "ResourceExplorer",
					"icon": "media/icon.svg",
					"contextualTitle": "ResourceExplorerTitle"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.65.0",
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.16.0",
		"@vscode/test-electron": "^2.1.3",
		"eslint": "^8.11.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"ts-loader": "^9.2.8",
		"typescript": "^4.5.5",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.9.2"
	}
}
